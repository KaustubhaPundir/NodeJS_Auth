{
    "version": 2,
    "builds": [
      {
     "src": "./app.js",
     "use": "@vercel/node"
      }
    ],
    "routes": [
      {
     "src": "/(.*)",
            "dest": "localhost:3000/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
      }
    ]
     }
